{
	"manifest_version": 3,
	"name": "Sheldon",
	"description": "Sheldon enables you to quickly and easily access the internet with just a single click.",
	"version": "0.1.0",
	"action": {
		"default_icon": {
			"16": "/Assets/Icons/icon-16.png",
			"24": "/Assets/Icons/icon-24.png",
			"32": "/Assets/Icons/icon-24.png",
			"48": "/Assets/Icons/icon-48.png",
			"128": "/Assets/Icons/icon-128.png"
		},
		"default_title": "Sheldon - Access the web with just a single click."
	},
	"icons": {
		"16": "/Assets/Icons/icon-16.png",
		"24": "/Assets/Icons/icon-24.png",
		"32": "/Assets/Icons/icon-24.png",
		"48": "/Assets/Icons/icon-48.png",
		"128": "/Assets/Icons/icon-128.png"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+U",
				"mac": "Command+U"
			}
		}
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["<all_urls>"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"author": "adityakrishnaoff@gmail.com",
	"permissions": ["activeTab", "storage", "cookies"],
	"host_permissions": ["https://user.heysheldon.com/"],
	"externally_connectable": {
		"matches": ["https://user.heysheldon.com/*"]
	},
	"web_accessible_resources": [
		{
			"resources": ["/Assets/*"],
			"matches": ["<all_urls>"],
			"use_dynamic_url": true
		}
	],
	"key": ""
}
