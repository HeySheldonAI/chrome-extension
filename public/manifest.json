{
	"manifest_version": 3,
	"name": "Sheldon",
	"description": "Sheldon enables you to quickly and easily access the internet with just a single click.",
	"version": "0.1.0",
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"24": "images/icon-24.png",
			"32": "images/icon-24.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		},
		"default_title": "Sheldon - Access the web with just a single click."
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "images/icon-16.png",
		"24": "images/icon-24.png",
		"32": "images/icon-24.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+U",
				"mac": "Command+U"
			}
		}
	},
	"author": "adityakrishnaoff@gmail.com",
	"permissions": ["activeTab", "scripting", "tabs"]
}
